{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/demo",
            "dest": "/demo_ui.html"
        },
        {
            "src": "/demo_ui.html",
            "dest": "/demo_ui.html"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task"
    },
    "functions": {
        "api/index.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}